{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "public": true,
  "sharable": "singleton",
  "requiredServices": [],
  "userInput": [
    {
      "name": "INTERFACE_NAME",
      "label": "The interface whose IP address should be spoken.",
      "type": "string",
      "defaultValue": "eth0"
    }
  ],
  "deployment": {
    "services": {
      "$SERVICE_NAME": {
        "image": "$SERVICE_CONTAINER"
      }
    }
  }
}

